{
    "os": "android",
    "arch": "arm64-v8a",
    "build_params": [
        "--android",
        "--android_sdk_path=/android_home",
        "--android_ndk_path=/ndk_home",
        "--android_abi=arm64-v8a",
        "--android_api=29",
        "--minimal_build",
        "--build_shared_lib",
        "--build_java",
        "--disable_ml_ops",
        "--disable_exceptions",
        "--cmake_extra_defines=onnxruntime_DISABLE_SPARSE_TENSORS=ON",
        "--cmake_extra_defines=onnxruntime_DISABLE_OPTIONAL_TYPE=ON",
        "--cmake_extra_defines=onnxruntime_DISABLE_ABSEIL=ON",
        "--include_ops_by_config=/onnxruntime_src/tools/ci_build/github/android/mobile_package.required_operators.config"
    ]
}
